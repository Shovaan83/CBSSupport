<?xml version="1.0" encoding="UTF-8" ?>
<Shell
    x:Class="CBSSupport.MAUIApp.AppShell"
    xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
    xmlns:local="clr-namespace:CBSSupport.MAUIApp"
    xmlns:views="clr-namespace:CBSSupport.MAUIApp.Views"
    Title="CBSSupport.MAUIApp">

    <TabBar>
        <ShellContent
            Title="Home"
            ContentTemplate="{DataTemplate local:MainPage}"
            Route="MainPage" />

        <ShellContent
            Title="Test"
            ContentTemplate="{DataTemplate views:TestPage}"
            Route="TestPage" />
    </TabBar>

</Shell>
